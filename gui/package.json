    "lint-fix": "eslint . --ext ts,tsx --fix",
    "static": "npm run prettier && npm run lint && npm run ts",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "tw": "tailwindcss ./src/globals.css -o ./out/tailwind.css && postcss --config postcss.config.js ./out/tailwind.css -o ./out/tailwind.css"
